<body class="text-center">
  <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
    <h1>The walk</h1>
    <table class='table table-striped'>
      <thead>
        <tr>
          <th>name</th>
          <th>description</th>
        </tr>
      </thead>
      <tbody>
        {{#each walks}}
        <tr>
          <td>{{this.name}}</td>
          <td>{{this.description}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</body>